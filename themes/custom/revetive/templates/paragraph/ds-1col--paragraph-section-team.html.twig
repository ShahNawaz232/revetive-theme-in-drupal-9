<div class="py-5 team4">
    <div class="container">
      <div class="row justify-content-center mb-4">
        <div class="col-md-7 text-center">
          <h3 class="mb-3">{{ content.field_team_subtitle }}</h3>
          <h6 class="subtitle">{{ content.field_team_title }}</h6>
        </div>
      </div>
      <div class="row">
        {% for key,value in content.field_team_name['#items'] %}
        <div class="col-lg-4 mb-4">
          <div class="row">
            <div class="col-md-12 col-sm-12 ">
             <div class="team-img"> {{ content.field_team_img[key] }} </div>
            </div>
            <div class="col-md-12 text-center">
              <div class="pt-2">
                <h5 class="mt-4 font-weight-medium mb-0">{{ content.field_team_name[key] }}</h5>
                <h6 class="subtitle mb-3">{{ content.field_team_body[key] }}</h6>
                <p>{{ content.field_team_des[key] }}</p>
              </div>
            </div>
          </div>
        </div>
        {% endfor %}
    </div>
  </div>